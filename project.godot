window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/github_to_itch/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[github_to_itch]

config/itch_username="Neverknow247"
config/itch_project_name="knockout-run"

